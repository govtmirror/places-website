    "copyright": "This project constitutes a work of the United States Government and is not subject to domestic copyright protection under 17 USC Section 105.",
    "attribution": "http://www.nps.gov"
  },
  "capabilities": {
    "version": {
      "maximum": "0.6",
      "minimum": "0.6"
    },
    "area": {
      "maximum": "0.25"
    },
    "tracepoints": {
      "per_page": "5000"
    },
    "waynodes": {
      "maximum": "50000"
    },
    "changesets": {
      "maximum_elements": "50000"
    },
    "timeout": {
      "seconds": "5"
    },
    "status": {
      "database": "online",
      "api": "online",
      "gpx": "online"
    }
  },
  "database": {
    "username": "postgres",
    "password": "postgres",
    "address": "localhost",
    "api": "poi_api",
    "pgs": "poi_pgs"
  },
  "iD": {
    "settings": {
      "connection": {
        "api": "ADDRESS FOR YOU SERVER",
      },
      "editing": {
        "area": true,
        "disabledFields": [
          "nps:places_uuid",
          "nps:building_id",
          "nps:fcat"
        ],
        "line": false,
        "minZoom": 15,
        "point": true
      },
      "locationOverlayMaxZoom": 22,
      "map": {
        "center": [-77.0228, 38.8944],
        "defaultBackground": "Esri",
        "zoom": 15.01
      },
      "tags": {
        "disabledFields": [
          "nps:building_id",
          "nps:fcat",
          "nps:places_id"
        ],
        "uninterestingFields": [
          "attribution",
          "created_by",
          "nps:unit_code",
          "odbl",
          "source"
        ]
      }
    }
  },
  "oauth": {
    "consumerKey": "consumerKey",
    "consumerSecret": "consumerSecret",
    "external": false,
    "local": {
      "auth": "/account/logon/",
      "server": "insidemaps.nps.gov"
    },
    "server": "www.openstreetmap.org"
  },
  "website": {
    "debug": true,
    "port": 80
  }
}
